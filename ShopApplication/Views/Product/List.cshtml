@model ProductViewModel
@{
    var popularClass = Model.Products.Count > 2 ? "popular" : "";
    var categoryName = Model.Category.Name;
    var products = Model.Products;
}
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
<style>
    .popular {
        color: darkslategray;
        font-weight: 700;
    }
</style>
<div class="navbar bg-dark navbar-dark navbar-expand-sm">
    <div class="container">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a href="#" class="nav-link">Link 1</a>
            </li>
            <li class="nav-item">
                <a href="#" class="nav-link">Link 2</a>
            </li>
            <li class="nav-item">
                <a href="#" class="nav-link">Link 3</a>
            </li>
        </ul>
    </div>
</div>
<header>
    <div class="jumbotron jumbotron-fluid">
        <div class="container">
            <h1 class="display-4">Shop App</h1>
            <p class="lead">Emre Gundogdu</p>
        </div>
    </div>
</header>
<main>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1 class="@popularClass">@categoryName</h1>
            </div>
        </div>

        @if (Model.Products.Count == 0)
        {
            <div class="row">
                <div class="col-md-12">
                    <div class="alert alert-danger">
                        Ürün yok
                    </div>
                </div>
            </div>

        }
        else
        {
            <div class="row">

                @foreach (var product in products)
                {
                    <div class="col-md-3">
                        <div class="card">
                            <img class="card-img-top" src="data:image/jpeg;base64," />
                            <div class="card-body">
                                <h5 class="card-title">@product.Name</h5>
                                <p class="card-text">@product.Description</p>
                                <a href="#" class="btn btn-dark">İncele</a>
                            </div>
                        </div>
                    </div>
                }
            </div>
        }
    </div>
</main>